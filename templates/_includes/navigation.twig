{% set navigations = [
  { url: 'demo/tabs', label: 'Tabs' },
  { url: 'demo/hideshow', label: 'Hide/Show' },
] %}
<div class="list-group">
  {% for item in navigations %}
  <a href="{{ url(item.url) }}" class="list-group-item list-group-item-action">
    {{ item.label }}
  </a>
  {% endfor %}
</div>